{
    "name": "nathanbate/laravel-contacts",
    "description": "A way to install contacts to your laravel project",
    "version": "1.0.0",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Nathanbate\\LaravelContacts\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Nathan Bate",
            "email": "noreply@noreply.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.36",
        "illuminate/support": "^11.36",
        "livewire/flux-pro": "^1.1",
        "livewire/livewire": "^3.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nathanbate\\LaravelContacts\\LaravelContactsServiceProvider"
            ],
            "dont-discover": []
        }
    },
    "repositories": {
        "flux-pro": {
            "type": "composer",
            "url": "https://composer.fluxui.dev"
        }
    }
}
